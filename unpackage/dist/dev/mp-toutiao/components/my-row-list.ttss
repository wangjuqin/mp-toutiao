
.contens{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 100%;
}
.navbar-content {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-webkit-flex-direction: row;
		        flex-direction: row;
		-webkit-box-pack: start;
		-webkit-justify-content: start;
		        justify-content: start;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		-webkit-flex-wrap: wrap;
		        flex-wrap: wrap;
		width: 95%;
}
.navbar-content .navbar-items {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		-webkit-flex-wrap: wrap;
		        flex-wrap: wrap;
		margin-bottom: 20rpx;
		width: calc(100% / 3 - 20rpx);
		margin: 20rpx 10rpx;
}
.navbar-content .navbar-items image {
		width: 100%;
		height: 175rpx;
}
.navbar-content .navbar-items .img {
		margin-top: 50rpx;
		width: 100%;
}
.navbar-content .navbar-items .img image {
		width: 100rpx;
		height: 100rpx;
		border-radius: 50%;
}
.navbar-content .navbar-items .name {
	margin-top: 6rpx;
		font-size: 29rpx;
		color: rgba(0, 0, 0, 1);
		width: 100%;
		text-overflow: ellipsis;
		overflow: hidden;
		word-break: break-all;
		white-space: nowrap;
}
.navbar-content .navbar-items .price {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: start;
		-webkit-justify-content: start;
		        justify-content: start;
		width: 100%;
		line-height: 50rpx;
}
.navbar-content .navbar-items .price view {
		text-overflow: ellipsis;
		overflow: hidden;
		word-break: break-all;
		white-space: nowrap;
		font-size: 35rpx;
		color: #FFAB18;
		font-weight: 550;
}
.navbar-content .navbar-items .price text {
		margin-left: 10rpx;
		font-size: 24rpx;
		color: rgba(0, 0, 0, 1);
}
.navbar-content .navbar-items .eye {
		width: 100%;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-webkit-flex-direction: row;
		        flex-direction: row;
		-webkit-box-pack: start;
		-webkit-justify-content: start;
		        justify-content: start;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		background-color: rgb(0, 0, 0, 0.3);
		/* opacity: 0.48; */
		margin-top: -35rpx;
		height: 35rpx;
}
.navbar-content .navbar-items .eye image {
		width: 32rpx;
		height: 20rpx;
}
.navbar-content .navbar-items .eye view {
		font-size: 20rpx;
		text-overflow: ellipsis;
		overflow: hidden;
		word-break: break-all;
		white-space: nowrap;
		color: #FFFFFF;
		margin-left: 5rpx;
}
.navbar-content .navbar-items .door {
		color: #383838 !important;
		font-size: 24rpx;
		opacity: .8;
}

