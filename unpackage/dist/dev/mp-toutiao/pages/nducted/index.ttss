.upload-image>image{
		background-color: #F2F2F2;
		width: 140rpx;
		height: 140rpx;
}
.upload-image .wx>image{
		background-color: #F2F2F2;
		width: 90rpx;
		height: 90rpx;
		padding: 50rpx;
}
.upload-image .mingping>image{
		background-color: #F2F2F2;
		width: 90rpx;
		height: 90rpx;
		padding: 50rpx;
}
.upload-image .wx>view{
			padding-top: 20rpx;
			width: 100%;
}
.upload-image .mingping>view{
			padding-top: 20rpx;
			width: 100%;
}
button{
			margin-top: 50rpx;
			background-color: #389BFE;
			color: #FFFFFF;
}
.upload-title{
			border: 0rpx solid #000000 !important;
			margin-top: 30rpx;
}
.fixed-box{
			position: fixed;
			width: 24%;
			bottom: 90rpx;
			right: 0rpx;
}
.fixed-box>image{
			width: 100%;
			height: 65rpx;
}
.active {
			color: #389BFE !important;
}


