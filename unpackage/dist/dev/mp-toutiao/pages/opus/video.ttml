<block tt:if="{{display==1}}"><view class="content"><block tt:if="{{isshow==0}}"><block><form class="box-form" style="width:95%;" id="forms"><input type="text" name="id" hidden="{{true}}" value="{{opuid}}"/><view class="title"><view>视频类型</view><view class="redioe"><view data-event-opts="{{[['tap',[['typeTop',[1]]]]]}}" class="{{(type==1?'actives':'')}}" bindtap="__e">楼盘直达</view><view data-event-opts="{{[['tap',[['typeTop',[2]]]]]}}" class="{{(type==2?'actives':'')}}" bindtap="__e">房产知识</view></view><input type="text" name="type" hidden="{{true}}" value="{{type}}"/></view><view class="title"><view>标题</view><input type="text" name="title" maxlength="40" data-event-opts="{{[['input',[['titleTap',['$event']]]]]}}" value="{{oputitle}}" bindinput="__e"/></view><block tt:if="{{type==1}}"><view class="select-house"><input type="text" name="properties_id" hidden="{{true}}" value="{{houseid}}"/><view>楼盘</view><view class="house">{{''+housname+''}}</view><button class="house-btn" type="primary" data-event-opts="{{[['tap',[['housebtn',['$event']]]]]}}" bindtap="__e">选择楼盘</button></view></block><view class="video"><input type="text" name="video" hidden="{{true}}" value="{{videourl}}"/><input type="text" name="image" hidden="{{true}}" value="{{thumburl}}"/><view>添加视频</view><view class="upload-video"><block tt:if="{{videourl}}"><video style="width:200rpx;height:200rpx;" src="{{$root.g0}}" controls="false" muted="{{true}}"></video></block><image class="upload-image" src="../../static/upload.png" mode data-event-opts="{{[['tap',[['uploads',['$event']]]]]}}" bindtap="__e"></image></view><view class="upload-tip">建议添加横屏视频</view></view><view class="sumbilt-button"><button class="hose-sumbilt" value="0" data-event-opts="{{[['tap',[['submitTap',[0]]]]]}}" bindtap="__e">发布</button><button class="hose-sumbilt" value="2" data-event-opts="{{[['tap',[['submitTap',[2]]]]]}}" bindtap="__e">保存为草稿</button></view></form></block></block><uni-popup class="vue-ref" vue-id="7415df01-1" type="top" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><view class="popupconten"><my-search style="width:100%;background-color:#EEF6FE;" bind:send="__e" vue-id="{{('7415df01-2')+','+('7415df01-1')}}" data-event-opts="{{[['^send',[['getCenters']]]]}}" bind:__l="__l"></my-search><block tt:if="{{Propertieslimit.length>0}}"><block><scroll-view style="width:100%;height:88%;" scroll-y="true" show-scrollbar="true" data-event-opts="{{[['scrolltolower',[['scrollBottom',['$event']]]]]}}" bindscrolltolower="__e"><block tt:for="{{Propertieslimit}}" tt:for-item="a" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['selHouseTop',['$0','$1'],[[['Propertieslimit','',index,'id']],[['Propertieslimit','',index,'title']]]]]]]}}" class="{{(('housename')+' '+(selecthouse==a.id?'active':''))}}" bindtap="__e">{{''+a.title+''}}</view></block></scroll-view></block></block><block tt:else><block><view class="housename">未找到楼盘，输入楼盘名称</view></block></block></view></uni-popup></view></block>