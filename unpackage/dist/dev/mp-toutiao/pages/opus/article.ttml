<block tt:if="{{display==1}}"><view class="container"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]],['reset',[['formReset',['$event']]]]]}}" class="box-form" style="width:95%;" bindsubmit="__e" bindreset="__e"><view class="title"><view>是否原创</view><view class="redioe"><view data-event-opts="{{[['tap',[['authorTop',[1]]]]]}}" class="{{(author==1?'active':'')}}" bindtap="__e">原创</view><view data-event-opts="{{[['tap',[['authorTop',[2]]]]]}}" class="{{(author==2?'active':'')}}" bindtap="__e">伪原创</view></view></view><view class="title"><view>标题</view><input type="text" name="title" maxlength="40" data-event-opts="{{[['input',[['titleTap',['$event']]]]]}}" value="{{oputitle}}" bindinput="__e"/></view><view class="page-body"><view class="wrapper"><view data-event-opts="{{[['tap',[['format',['$event']]]]]}}" class="toolbar" bindtap="__e"><view class="{{(('iconfont icon-zitijiacu')+' '+(formats.bold?'ql-active':''))}}" data-name="bold"></view><view class="{{(('iconfont icon-zitixieti')+' '+(formats.italic?'ql-active':''))}}" data-name="italic"></view><view class="{{(('iconfont icon-zitixiahuaxian')+' '+(formats.underline?'ql-active':''))}}" data-name="underline"></view><view class="{{(('iconfont icon-zitishanchuxian')+' '+(formats.strike?'ql-active':''))}}" data-name="strike"></view><view class="{{(('iconfont icon-zuoduiqi')+' '+(formats.align==='left'?'ql-active':''))}}" data-name="align" data-value="left"></view><view class="{{(('iconfont icon-juzhongduiqi')+' '+(formats.align==='center'?'ql-active':''))}}" data-name="align" data-value="center"></view><view class="{{(('iconfont icon-youduiqi')+' '+(formats.align==='right'?'ql-active':''))}}" data-name="align" data-value="right"></view><view class="{{(('iconfont icon-zuoyouduiqi')+' '+(formats.align==='justify'?'ql-active':''))}}" data-name="align" data-value="justify"></view><view class="{{(('iconfont icon-line-height')+' '+(formats.lineHeight?'ql-active':''))}}" data-name="lineHeight" data-value="2"></view><view class="{{(('iconfont icon-Character-Spacing')+' '+(formats.letterSpacing?'ql-active':''))}}" data-name="letterSpacing" data-value="2em"></view><view class="{{(('iconfont icon-722bianjiqi_duanqianju')+' '+(formats.marginTop?'ql-active':''))}}" data-name="marginTop" data-value="20px"></view><view class="{{(('iconfont icon-723bianjiqi_duanhouju')+' '+(formats.previewarginBottom?'ql-active':''))}}" data-name="marginBottom" data-value="20px"></view><view data-event-opts="{{[['tap',[['removeFormat',['$event']]]]]}}" class="iconfont icon-clearedformat" bindtap="__e"></view><view class="{{(('iconfont icon-font')+' '+(formats.fontFamily?'ql-active':''))}}" data-name="fontFamily" data-value="Pacifico"></view><view class="{{(('iconfont icon-fontsize')+' '+(formats.fontSize==='24px'?'ql-active':''))}}" data-name="fontSize" data-value="24px"></view><view class="{{(('iconfont icon-text_color')+' '+(formats.color==='#0000ff'?'ql-active':''))}}" data-name="color" data-value="#0000ff"></view><view class="{{(('iconfont icon-fontbgcolor')+' '+(formats.backgroundColor==='#00ff00'?'ql-active':''))}}" data-name="backgroundColor" data-value="#00ff00"></view><view data-event-opts="{{[['tap',[['insertDate',['$event']]]]]}}" class="iconfont icon-date" bindtap="__e"></view><view class="iconfont icon--checklist" data-name="list" data-value="check"></view><view class="{{(('iconfont icon-youxupailie')+' '+(formats.list==='ordered'?'ql-active':''))}}" data-name="list" data-value="ordered"></view><view class="{{(('iconfont icon-wuxupailie')+' '+(formats.list==='bullet'?'ql-active':''))}}" data-name="list" data-value="bullet"></view><view data-event-opts="{{[['tap',[['undo',['$event']]]]]}}" class="iconfont icon-undo" bindtap="__e"></view><view data-event-opts="{{[['tap',[['redo',['$event']]]]]}}" class="iconfont icon-redo" bindtap="__e"></view><view class="iconfont icon-outdent" data-name="indent" data-value="-1"></view><view class="iconfont icon-indent" data-name="indent" data-value="+1"></view><view data-event-opts="{{[['tap',[['insertDivider',['$event']]]]]}}" class="iconfont icon-fengexian" bindtap="__e"></view><view data-event-opts="{{[['tap',[['insertImage',['$event']]]]]}}" class="iconfont icon-charutupian" bindtap="__e"></view><view class="{{(('iconfont icon-format-header-1')+' '+(formats.header===1?'ql-active':''))}}" data-name="header" data-value="{{1}}"></view><view class="{{(('iconfont icon-zitixiabiao')+' '+(formats.script==='sub'?'ql-active':''))}}" data-name="script" data-value="sub"></view><view class="{{(('iconfont icon-zitishangbiao')+' '+(formats.script==='super'?'ql-active':''))}}" data-name="script" data-value="super"></view><view data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="iconfont icon-shanchu" bindtap="__e"></view><view class="{{(('iconfont icon-direction-rtl')+' '+(formats.direction==='rtl'?'ql-active':''))}}" data-name="direction" data-value="rtl"></view></view><editor class="ql-container" id="editor" value="{{content}}" name="content" placeholder="文章内容" showImgSize="{{true}}" showImgToolbar="{{true}}" showImgResize="{{true}}" read-only="{{readOnly}}" data-event-opts="{{[['statuschange',[['onStatusChange',['$event']]]],['ready',[['onEditorReady',['$event']]]],['input',[['contenttop',['$event']]]]]}}" bindstatuschange="__e" bindready="__e" bindinput="__e"></editor></view></view><view class="video"><input type="text" name="image" hidden="{{true}}" value="{{imageurl}}"/><view>添加图片</view><view class="images"><block tt:if="{{thumburl}}"><image style="width:200rpx;height:200rpx;" src="{{thumburl}}"></image></block><view class="upload-video"><image src="../../static/upload.png" mode data-event-opts="{{[['tap',[['uploads',['$event']]]]]}}" bindtap="__e"></image></view></view></view><view class="sumbilt-button"><button class="hose-sumbilt" value="0" data-event-opts="{{[['tap',[['submitTap',[0]]]]]}}" bindtap="__e">发布</button><button class="hose-sumbilt" value="2" data-event-opts="{{[['tap',[['submitTap',[2]]]]]}}" bindtap="__e">保存为草稿</button></view></form></view></block>