<view class="uni"><block tt:for="{{listFollow}}" tt:for-item="a" tt:for-index="item" tt:key="item"><view class="advise"><view class="item"><view data-event-opts="{{[['tap',[['detailstop',['$0'],[[['listFollow','',item,'user_id']]]]]]]}}" class="left" bindtap="__e"><block tt:if="{{a.avatar}}"><image src="{{a.avatar}}"></image></block><block tt:else><image src="../../static/img.png"></image></block><block tt:if="{{a.name}}"><view class="uname">{{a.name}}</view></block></view><view class="right"><image src="../../static/MSG.png" data-event-opts="{{[['tap',[['seekTop',['$0'],[[['listFollow','',item,'id']]]]]]]}}" bindtap="__e"></image><image src="../../static/tel.png" data-event-opts="{{[['tap',[['teltop',['$0'],[[['listFollow','',item,'phone']]]]]]]}}" bindtap="__e"></image></view></view><view class="bottom"><view class="preview"><block tt:if="{{a.click}}"><text>{{a.click}}</text></block>浏览量</view><view class="activity"><block tt:if="{{a.activity}}"><text>{{''+a.activity}}</text></block>活跃度</view><view class="likes"><block tt:if="{{a.likes}}"><text>{{a.likes}}</text></block>点赞数</view></view></view></block><view class="flex-box"><image src="../../static/home_btn.png" mode data-event-opts="{{[['tap',[['Hometop',['$event']]]]]}}" bindtap="__e"></image></view></view>