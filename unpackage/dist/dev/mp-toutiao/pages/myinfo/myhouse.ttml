<view class="content"><view class="navbar-top"><view>主营楼盘</view><block tt:if="{{shoptype==1}}"><view data-event-opts="{{[['tap',[['addhouse',['$event']]]]]}}" class="img" bindtap="__e">添加<image src="../../static/upload.png" mode></image></view></block></view><view class="navbar-content-column"><block tt:for="{{$root.l0}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="navbar-item"><view data-event-opts="{{[['tap',[['housedetail',['$0'],[[['listHouse','',index,'id']]]]]]]}}" class="item-left" bindtap="__e"><block tt:if="{{item.$orig.image}}"><image src="{{item.g0}}"></image></block><block tt:else><image src="../../static/img.png"></image></block><block tt:if="{{item.$orig.click!=null}}"><view class="eye"><image src="../../static/view.png"></image><view>{{item.$orig.click+''}}</view></view></block></view><view class="item-rigth"><view data-event-opts="{{[['tap',[['housedetail',['$0'],[[['listHouse','',index,'id']]]]]]]}}" class="name" bindtap="__e"><view>{{item.$orig.title}}</view></view><view data-event-opts="{{[['tap',[['housedetail',['$0'],[[['listHouse','',index,'id']]]]]]]}}" class="info" bindtap="__e">{{item.$orig.region_title}}<block tt:if="{{item.$orig.apartment}}"><text>{{"/"+item.$orig.apartment+"居室"}}</text></block></view><view data-event-opts="{{[['tap',[['housedetail',['$0'],[[['listHouse','',index,'id']]]]]]]}}" class="price" bindtap="__e"><view>{{item.$orig.price}}</view><text>元/㎡</text></view><view class="label"><block tt:if="{{item.$orig.sale}}"><view class="{{(item.$orig.sale=='在售'?'zs':'ds')}}">{{item.$orig.sale}}</view></block><block tt:if="{{item.$orig.type}}"><view class="type">{{item.$orig.type}}</view></block><block tt:if="{{shoptype==1}}"><image src="../../static/delect.png" data-event-opts="{{[['tap',[['delecthouse',['$0'],[[['listHouse','',index,'id']]]]]]]}}" bindtap="__e"></image></block></view></view></view></block></view></view>