.list .list-item .label {
	margin-top: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: start;
	-webkit-justify-content: start;
	        justify-content: start;
	/* width: 35%; */
	font-size: 24rpx;
}
.list .list-item .label>view {
	background-color: #D5D7E6;
	padding: 5rpx 30rpx;
	border-radius: 50rpx;
	margin: 5px;
}
.list .list-item .nums {
	margin-top: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: start;
	-webkit-justify-content: start;
	        justify-content: start;
	/* width: 30%; */
	font-size: 24rpx;
	color: #6F6F6F;
}
.list .list-item .nums>view {
	margin: 10rpx;
}
.list .list-item .uni-tag {
	width: 12%;
	font-size: 23rpx;
	background-color: #FF2C29;
	color: #FFFFFF;
	text-align: center;
	line-height: 34rpx;
}
.img {
	width: 95%;
	text-align: center;
	margin: 80rpx 0rpx;
}
.img>image {
	width: 340rpx;
	height: 200rpx;
}
button {
