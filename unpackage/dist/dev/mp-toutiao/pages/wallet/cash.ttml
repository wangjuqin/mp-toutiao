<view class="content"><block tt:if="{{amount}}"><view class="uni-card"><view class="headerimg"><image src="../../static/money.png" mode></image><view>{{''+amount.balance+' 元'}}</view></view><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]],['reset',[['formReset',['$event']]]]]}}" class="form-card" bindsubmit="__e" bindreset="__e"><view class="labertitle">提现金额</view><view class="laber-input"><view>￥</view><input type="number" name="amount" placeholder="{{amount.balance}}" value="{{balance}}"/><view data-event-opts="{{[['tap',[['totalTop',['$event']]]]]}}" bindtap="__e">全部</view></view><view class="labertitle">银行卡号</view><view class="laber-input"><view></view><input type="number" name="card" placeholder="银行卡号" value="{{banks.card}}"/><view></view></view><view class="labertitle">开户名</view><view class="laber-input"><view></view><input type="text" name="name" placeholder="开户名" value="{{banks.name}}"/><view></view></view><view class="labertitle">开户行</view><view class="laber-input"><view></view><input type="text" name="bank" placeholder="开户行" value="{{banks.bank}}"/><view></view></view><button class="card-bottom" form-type="submit">提现</button></form></view></block></view>